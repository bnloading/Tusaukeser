{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/images/(.*).png",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable",
                "content-type": "image/png"
            },
            "dest": "/images/$1.png"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}